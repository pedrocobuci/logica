Algoritmo "troca de numero"

Var
    n1, n2, troca : real


Inicio
    escreval("digite dois numeros : ")
    leia(n1)
    
    escreval("digite dois numeros : ")
    leia(n2)
    
    troca <- n1

    n1 <- n2

    n2 <- troca

escreval ("O novo valor de A é: ", n1)

escreval ("O novo valor de B é: ", n2)

Fimalgoritmo