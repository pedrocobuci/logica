Algoritmo "velocidade"

Var
  vel, m, vm : real

Inicio
        escreva("digite a velocidade do veículo: ")
        leia(vel)
    
      vm<- 433.4 / vel
    
      m<- vel / 3.6
    
        escreval("o percursso de São Paulo até o Rio de Janeiro vai durar: ", vm, " hrs")
        escreval("velocidade em m/s:", m)
    
Fimalgoritmo